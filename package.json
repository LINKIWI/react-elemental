{
  "name": "react-elemental",
  "version": "1.2.2",
  "description": "Flat UI library for React",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf lib",
    "build": "babel --verbose src/ -d lib/",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls",
    "prepublish": "npm run clean && npm run build && derive-pkg -d lib",
    "publish": "npm publish lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LINKIWI/react-elemental.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@linkiwi/hoc": "0.0.2",
    "color": "3.1.2",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@linkiwi/eslint-config": "2.0.2",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "concurrently": "4.1.0",
    "coveralls": "3.0.4",
    "derive-pkg-cli": "1.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-react": "7.13.0",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "pre-commit": "1.2.2",
    "raf": "3.4.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6",
    "sinon": "7.3.2"
  },
  "peerDependencies": {
    "react": "^16.4.0"
  }
}
